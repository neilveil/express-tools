{
  "scripts": {
    "build": "rimraf build && tsc && cp package.public.json build/package.json",
    "pretty": "prettier --config .prettierrc --write \"./**/*.js\"",
    "lint": "eslint . --ext .ts",
    "jest": "NODE_ENV=et-test jest",
    "jest:watch": "NODE_ENV=et-test jest --watchAll --runInBand",
    "test": "npm run lint && npm run jest",
    "precommit": "npm run pretty && npm run test",
    "prepare": "husky install",
    "publish": "npm run build && cd build && npm publish"
  },
  "dependencies": {
    "ajv": "^8.6.3",
    "axios": "^0.21.1",
    "chalk": "^4.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "joi": "^17.4.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.1",
    "@types/cors": "^2.8.12",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "eslint": "^7.30.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "node-ts": "^5.1.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
