{
  "name": "express-tools",
  "description": "The Server Wizard",
  "version": "2.1.5",
  "author": "neilveil",
  "repository": "neilveil/express-tools",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "cd demo && nodemon --ignore . src/index.ts",
    "dist": "rimraf dist && tsc",
    "pretty": "prettier --write ./src/**/*.ts",
    "jest:app": "jest -i src/app/index.test.ts --coverage",
    "jest:crypto": "jest -i src/crypto/index.test.ts --coverage",
    "jest:env": "jest -i src/env/index.test.ts --coverage",
    "jest:response": "jest -i src/response/index.test.ts --coverage",
    "jest:server": "jest -i src/server/index.test.ts --coverage",
    "jest:tdb": "jest -i src/tdb/index.test.ts --coverage",
    "jest:validate": "jest -i src/validate/index.test.ts --coverage",
    "jest:watch": "jest --watchAll --runInBand",
    "jest": "jest --coverage",
    "test": "npm run jest",
    "lint": "eslint . --ext ts",
    "prepare": "husky install",
    "prepublishOnly": "npm run test"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "axios": "^1.4.0",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "joi": "^17.9.2"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/mocha": "^10.0.3",
    "@types/node": "^20.5.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "husky": "^8.0.3",
    "jest": "^29.6.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.1.6"
  },
  "keywords": [
    "api",
    "backend",
    "caching",
    "development",
    "encryption",
    "express",
    "expressjs",
    "framework",
    "helpers",
    "logging",
    "server",
    "validation"
  ]
}
